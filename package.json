{
  "name": "todo",
  "version": "1.0.0",
  "description": "react, redux, typescript test app",
  "main": "index.js",
  "scripts": {
    "watch:dev": "better-npm-run watch:dev",
    "build:dev": "better-npm-run build:dev",
    "dev": "node_modules/.bin/concurrently  -k \"npm run start-server\" \"npm run watch:dev\""
  },
  "betterScripts": {
    "watch:dev": {
      "command": "node_modules/.bin/webpack-dev-server --watch --progress --colors --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:dev": {
      "command": "node_modules/.bin/webpack --progress --colors --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clicktronix/react-redux-TODO.git"
  },
  "keywords": [
    "react",
    "redux",
    "typescript"
  ],
  "author": "Vladislav Manakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clicktronix/react-redux-TODO/issues"
  },
  "homepage": "https://github.com/clicktronix/react-redux-TODO#readme",
  "dependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "axios": "^0.16.1",
    "bem-cn": "^2.1.3",
    "better-npm-run": "0.0.15",
    "css-loader": "^0.28.0",
    "decko": "^1.2.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "normalizr": "^3.2.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-order": "^0.4.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tslint": "^5.1.0",
    "tslint-eslint-rules": "^4.0.0",
    "tslint-react": "^2.6.0",
    "typescript": "^2.2.2",
    "typings": "^2.1.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
